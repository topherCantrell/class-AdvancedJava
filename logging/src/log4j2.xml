<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="info">
  <Appenders>
    <Console name="To The Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  
  <Loggers>  
  
    <Root level="trace">
      <AppenderRef ref="To The Console"/>
    </Root> 
             
  </Loggers>  
</Configuration>

